    "file-loader": "1.1.5",
    "format-duration": "^1.3.1",
    "formik": "^1.3.0",
    "fs-extra": "3.0.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "lodash.merge": "^4.6.1",
    "object-assign": "4.1.1",
    "phoenix": "^1.4.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
